<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
	<!--
		Configuration file for UrlRewriteFilter http://tuckey.org/urlrewrite/
	-->
<urlrewrite>
	<rule>
		<from>^/trackedremote/(\d*[0-9](|.\d*[0-9]|,\d*[0-9])?)/(\d*[0-9](|.\d*[0-9]|,\d*[0-9])?)/([0-9]+)$</from>
		<to>/trackedremote/?lat=$1&amp;lng=$3&amp;trackid=$5</to>
            <condition type="session-attribute" name="certificated" operator="equal">true</condition>
        </rule>
        <rule>
            <from>^/trackedremote/(\d*[0-9](|.\d*[0-9]|,\d*[0-9])?),(\d*[0-9](|.\d*[0-9]|,\d*[0-9])?)/([0-9]+)$</from>
            <to>/trackedremote/?lat=$1&amp;lng=$3&amp;trackid=$5</to>
            <condition type="session-attribute" name="certificated" operator="equal">true</condition>
        </rule>
        <rule>
            <from>^/trackedremote/newtrack$</from>
            <to>/trackedremote/?newtrack=true</to>
            <condition type="session-attribute" name="certificated" operator="equal">true</condition>
        </rule>
        <rule>
            <from>^/en/(.*)$</from>
            <to>/$1?locale=en</to>
        </rule>
        <rule>
            <from>^/vn/(.*)$</from>
            <to>/$1?locale=vi</to>
        </rule>
        <rule>
            <from>/vi/</from>
            <to>/?locale=vi</to>
        </rule>
</urlrewrite>

